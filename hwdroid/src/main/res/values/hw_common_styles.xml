<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <style name="Widget.Ali.Button">
        <item name="android:background">@drawable/hw_btn_default</item>
        <item name="android:textAppearance">@style/TextAppearance.Ali.Widget.Button</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <!--
        button text
        <item name="android:textSize">@dimen/hw_button_text_size</item>
        -->
    </style>

    <style name="Widget.Ali.Button.Small">
        <item name="android:background">@drawable/hw_btn_default_small</item>
        <item name="android:textAppearance">@style/TextAppearance.Ali.Widget.Button.Small</item>
    </style>

    <style name="Widget.Ali.Button.Inset" parent="android:style/Widget.Holo.Button.Inset"></style>

    <style name="Widget.Ali.Button.Toggle" parent="@android:style/Widget.Holo.Button.Toggle">
        <item name="android:textAppearance">@style/TextAppearance.Ali.Small</item>
    </style>

    <style name="Widget.Ali.Button.Dialog">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bar_bg</item>
        <item name="android:minHeight">@dimen/hw_alert_dialog_button_bar_height</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>

    <style name="Widget.Ali.Button.Dialog.Left">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>

    <style name="Widget.Ali.Button.Dialog.Middle">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_cancel_button_text_color</item>
    </style>


    <style name="Widget.Ali.Button.Dialog.Right">
        <item name="android:background">@drawable/hw_alert_dialog_btn_bg</item>
        <item name="android:textColor">@color/hw_dialog_ok_button_text_color</item>
    </style>


    <style name="Widget.Ali.TextView">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textSelectHandleLeft">?android:attr/textSelectHandleLeft</item>
        <item name="android:textSelectHandleRight">?android:attr/textSelectHandleRight</item>
        <item name="android:textSelectHandle">?android:attr/textSelectHandle</item>
        <item name="android:textEditPasteWindowLayout">?android:attr/textEditPasteWindowLayout
        </item>
        <item name="android:textEditNoPasteWindowLayout">?android:attr/textEditNoPasteWindowLayout
        </item>
        <item name="android:textEditSidePasteWindowLayout">
            ?android:attr/textEditSidePasteWindowLayout
        </item>
        <item name="android:textEditSideNoPasteWindowLayout">
            ?android:attr/textEditSideNoPasteWindowLayout
        </item>
        <item name="android:textEditSuggestionItemLayout">
            ?android:attr/textEditSuggestionItemLayout
        </item>
        <item name="android:textCursorDrawable">?android:attr/textCursorDrawable</item>
    </style>

    <style name="Widget.Ali.CheckedTextView"></style>

    <style name="Widget.Ali.EditText" parent="Widget.Ali.TextView">
        <item name="android:background">@drawable/hw_edit_text</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">?android:attr/editTextColor</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Widget.Ali.AutoCompleteTextView" parent="Widget.Ali.EditText">
        <item name="android:completionHintView">@layout/hw_simple_dropdown_hint</item>
        <item name="android:completionThreshold">2</item>
        <item name="android:dropDownVerticalOffset">-6dip</item>
        <item name="android:dropDownHorizontalOffset">0dip</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:dropDownSelector">@drawable/hw_list_selector_background</item>
        <item name="android:popupBackground">@drawable/hw_menu_dropdown_panel</item>
    </style>

    <style name="Widget.Ali.CompoundButton">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="android:textColor">?android:attr/textColorPrimaryDisableOnly</item>
        <item name="android:gravity">center_vertical|start</item>
    </style>

    <style name="Widget.Ali.CompoundButton.CheckBox">
        <item name="android:button">?android:attr/listChoiceIndicatorMultiple</item>
    </style>

    <style name="Widget.Ali.AbsListView">
        <item name="android:scrollbars">vertical</item>
        <item name="android:fadingEdge">vertical</item>
    </style>

    <style name="Widget.Ali.ListView" parent="Widget.Ali.AbsListView">
        <item name="android:background">@drawable/hw_listview_background</item>
        <item name="android:divider">?android:attr/listDivider</item>
        <item name="android:listSelector">?android:attr/listChoiceBackgroundIndicator</item>
        <item name="android:cacheColorHint">?android:attr/colorBackgroundCacheHint</item>
        <!-- <item name="android:scrollbarThumbVertical">@drawable/hw_list_divider</item> -->
    </style>

    <style name="Widget.Ali.ListView.White" parent="Widget.Ali.ListView"></style>

    <style name="Widget.Ali.ListView.DropDown">
        <item name="android:cacheColorHint">@null</item>
        <!-- <item name="android:divider">?android:attr/listChoiceBackgroundIndicator</item> -->
    </style>

    <style name="Widget.Ali.ExpandableListView" parent="Widget.Ali.ListView">
        <item name="android:groupIndicator">@drawable/hw_list_expander_group</item>
        <item name="android:childDivider">@drawable/hw_list_divider</item>
        <!--
            indicatorLeft and indicatorRight's format is dimension,
            but android hide these style in themes file.
            We can write here for this style been derived from Ali.ListView other than android expandablelistview.
        -->
        <item name="android:indicatorLeft">?android:attr/expandableListPreferredItemIndicatorLeft
        </item>
        <item name="android:indicatorRight">
            ?android:attr/expandableListPreferredItemIndicatorRight
        </item>
    </style>

    <style name="Widget.Ali.GridView" parent="android:style/Widget.Holo.GridView"></style>

    <style name="Widget.Ali.ImageButton" parent="android:style/Widget.Holo.ImageButton"></style>

    <style name="Widget.Ali.PopupWindow" parent="android:style/Widget.Holo.PopupWindow"></style>

    <style name="Widget.Ali.ProgressBar">
        <item name="android:indeterminateOnly">true</item>
        <!-- <item name="android:indeterminateDrawable">@android:drawable/progress_medium_white</item> -->
        <item name="android:indeterminateDrawable">@anim/hw_progress_anim</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDuration">3500</item>
        <item name="android:minWidth">@dimen/hw_progressbar_normal_min_width</item>
        <item name="android:maxWidth">@dimen/hw_progressbar_normal_max_width</item>
        <item name="android:minHeight">@dimen/hw_progressbar_normal_min_height</item>
        <item name="android:maxHeight">@dimen/hw_progressbar_normal_max_height</item>
        <!-- <item name="android:mirrorForRtl">false</item> -->
    </style>

    <style name="Widget.Ali.ProgressBar.Inverse">
        <item name="android:indeterminateDrawable">@anim/hw_progress_inverse_anim</item>
    </style>

    <style name="Widget.Ali.ProgressBar.Large">
        <item name="android:indeterminateDrawable">@anim/hw_progress_big_anim</item>
        <item name="android:minWidth">@dimen/hw_progressbar_large_min_width</item>
        <item name="android:maxWidth">@dimen/hw_progressbar_large_max_width</item>
        <item name="android:minHeight">@dimen/hw_progressbar_large_min_height</item>
        <item name="android:maxHeight">@dimen/hw_progressbar_large_max_height</item>
    </style>

    <style name="Widget.Ali.ProgressBar.Large.Inverse">
        <item name="android:indeterminateDrawable">@anim/hw_progress_big_inverse_anim</item>
    </style>

    <style name="Widget.Ali.ProgressBar.Horizontal" parent="android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/hw_progress_horizontal</item>
        <item name="android:indeterminateDrawable">@drawable/hw_progress_indeterminate_horizontal
        </item>
        <item name="android:minHeight">@dimen/hw_progressbar_horizontal_min_height</item>
        <item name="android:maxHeight">@dimen/hw_progressbar_horizontal_max_height</item>
    </style>

    <style name="Widget.Ali.ProgressBar.Small" parent="android:style/Widget.Holo.ProgressBar.Small"></style>

    <style name="Widget.Ali.ProgressBar.Small.Title" parent="android:style/Widget.Holo.ProgressBar.Small.Title"></style>

    <style name="Widget.Ali.SeekBar" parent="android:style/Widget.Holo.SeekBar">
        <item name="android:minHeight">4dip</item>
        <item name="android:maxHeight">4dip</item>
        <item name="android:thumb">@drawable/hw_scrubber_control_selector</item>
        <item name="android:progressDrawable">@drawable/hw_scrubber_progress_horizontal</item>
        <item name="android:indeterminateDrawable">@drawable/hw_scrubber_progress_horizontal</item>
    </style>

    <style name="Widget.Ali.SeekBar.NoProColor" parent="android:style/Widget.Holo.SeekBar">
        <item name="android:minHeight">4dip</item>
        <item name="android:maxHeight">4dip</item>
        <item name="android:thumb">@drawable/hw_scrubber_control_selector</item>
        <item name="android:progressDrawable">@drawable/hw_scrubber_progress_horizontal_noprocolor
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/hw_scrubber_progress_horizontal_noprocolor
        </item>
    </style>

    <style name="Widget.Ali.RatingBar" parent="android:style/Widget.Holo.RatingBar"></style>

    <style name="Widget.Ali.RatingBar.Indicator" parent="android:style/Widget.Holo.RatingBar.Indicator"></style>

    <style name="Widget.Ali.RatingBar.Small" parent="android:style/Widget.Holo.RatingBar.Small"></style>

    <style name="Widget.Ali.CompoundButton.RadioButton" parent="android:style/Widget.Holo.CompoundButton.RadioButton"></style>

    <style name="Widget.Ali.ScrollView" parent="android:style/Widget.Holo.ScrollView"></style>

    <style name="Widget.Ali.HorizontalScrollView" parent="android:style/Widget.Holo.HorizontalScrollView"></style>

    <style name="Widget.Ali.Spinner">
        <item name="android:background">@drawable/hw_spinner_background</item>
        <item name="android:clickable">true</item>
        <item name="android:spinnerMode">dialog</item>
        <item name="android:dropDownSelector">@drawable/hw_list_selector_background</item>
        <item name="android:popupBackground">@drawable/hw_spinner_dropdown_bg</item>
        <item name="android:dropDownVerticalOffset">0dip</item>
        <item name="android:dropDownHorizontalOffset">0dip</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>

    <style name="Widget.Ali.Spinner.DropDown">
        <item name="android:spinnerMode">dropdown</item>
    </style>

    <style name="Widget.Ali.CompoundButton.Star" parent="android:style/Widget.Holo.CompoundButton.Star"></style>

    <style name="Widget.Ali.TabWidget">
        <item name="android:textAppearance">@style/TextAppearance.Ali.Widget.TabWidget</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:tabStripLeft">@drawable/hw_tab_unselected</item>
        <item name="android:tabStripRight">@drawable/hw_tab_unselected</item>
        <item name="android:tabStripEnabled">true</item>
        <item name="android:divider">@null</item>
        <item name="android:gravity">fill_horizontal|center_vertical</item>
        <!--<item name="*android:tabLayout">@layout/hw_tab_indicator</item>-->
    </style>

    <style name="Widget.Ali.WebTextView" parent="android:style/Widget.Holo.WebTextView"></style>

    <style name="Widget.Ali.WebView" parent="android:style/Widget.Holo.WebView"></style>

